from random import randint

a = [[0 for x in range(10)] for x in range(10)]

sum = 0

for i in range(0,10):
    for j in range(0,10):
        temp = randint(0,9)
        a[i][j] = temp
        sum += temp

for i in range(0,10):
    for j in range(0,10):
        print(a[i][j], " ", end='')
    print()

print("sum is " + str(sum))
